    "\n",
    "url = \"http://127.0.0.1:8000/sum\"\n",
    "payload = {\"a\": 310, \"b\": 25}\n",
    "\n",
    "response = requests.post(url, json=payload)\n",
    "\n",
    "if response.ok:\n",
    "    print(\"Result:\", response.json()[\"result\"])\n",
    "else:\n",
    "    print(\"Error:\", response.status_code, response.text)\n"
